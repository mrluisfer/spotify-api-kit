{
	"compilerOptions": {
		"target": "es2016",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"outDir": "./dist",
		"declaration": true,
		"rootDir": "./src",
		"declarationMap": true,
		"emitDeclarationOnly": false,
		"module": "nodenext",
		"moduleResolution": "nodenext"
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
}
